/ {
    behaviors {
        ytmpp: ytmusic_play_pause {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp C_PP>, <&kp F21>;
            mods = <(MOD_LCTL|MOD_LSFT|MOD_RCTL|MOD_RSFT)>;
        };
		ytmprev: ytmusic_previous {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp C_PREV>, <&kp F19>;
            mods = <(MOD_LCTL|MOD_LSFT|MOD_RCTL|MOD_RSFT)>;
        };
		ytmnext: ytmusic_next {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp C_NEXT>, <&kp F22>;
            mods = <(MOD_LCTL|MOD_LSFT|MOD_RCTL|MOD_RSFT)>;
        };
    };
};

/ {
    conditional_layers {
        compatible = "zmk,conditional-layers";
        tri_layer {
            if-layers = <MIRROR ARACTV>;
            then-layer = <ARROWL>;
        };
    };
};

/ {
    macros {
        lm: lm {
			compatible = "zmk,behavior-macro-two-param";
			wait-ms = <0>;
			tap-ms = <0>;
			#binding-cells = <2>;
			bindings
				= <&macro_param_1to1>
				, <&macro_press &mo MACRO_PLACEHOLDER>
				, <&macro_param_2to1>
				, <&macro_press &kp MACRO_PLACEHOLDER>
				, <&macro_pause_for_release>
				, <&macro_param_2to1>
				, <&macro_release &kp MACRO_PLACEHOLDER>
				, <&macro_param_1to1>
				// if all modifiers are released
					, <&macro_release &mo MACRO_PLACEHOLDER>
				// else
				// return
				;
		};
		squared: squared {
			compatible = "zmk,behavior-macro";
			wait-ms = <40>;
			tap-ms = <40>;
			#binding-cells = <0>;
			bindings
				= <&macro_press   &kp LCTRL &kp LSHIFT>
				, <&macro_tap     &kp U>
				, <&macro_release &kp LCTRL &kp LSHIFT>
				, <&macro_tap     &kp KP_N0 &kp KP_N0 &kp B &kp KP_N2 &kp SPACE>
			;
		};
		squaredold: squaredold {
			compatible = "zmk,behavior-macro";
			wait-ms = <40>;
			tap-ms = <40>;
			#binding-cells = <0>;
			bindings
				= <&macro_press   &kp LALT>
				, <&macro_tap     &kp KP_N0 &kp KP_N1 &kp KP_N7 &kp KP_N8>
				, <&macro_release &kp LALT>
			;
		};
		cubed: cubed {
			compatible = "zmk,behavior-macro";
			wait-ms = <40>;
			tap-ms = <40>;
			#binding-cells = <0>;
			bindings
				= <&macro_press   &kp LCTRL &kp LSHIFT>
				, <&macro_tap     &kp U>
				, <&macro_release &kp LCTRL &kp LSHIFT>
				, <&macro_tap     &kp KP_N0 &kp KP_N0 &kp B &kp KP_N3 &kp SPACE>
				;
		};
		cubedold: cubedold {
			compatible = "zmk,behavior-macro";
			wait-ms = <40>;
			tap-ms = <40>;
			#binding-cells = <0>;
			bindings
				= <&macro_press   &kp LALT>
				, <&macro_tap     &kp KP_N0 &kp KP_N1 &kp KP_N7 &kp KP_N9>
				, <&macro_release &kp LALT>
				;
		};
        degree: degree {
			compatible = "zmk,behavior-macro";
			wait-ms = <40>;
			tap-ms = <40>;
			#binding-cells = <0>;
			bindings
				= <&macro_press   &kp LCTRL &kp LSHIFT>
				, <&macro_tap     &kp U>
				, <&macro_release &kp LCTRL &kp LSHIFT>
				, <&macro_tap     &kp KP_N0 &kp KP_N0 &kp B &kp KP_N0 &kp SPACE>
				;
		};
		degreeold: degreeold {
			compatible = "zmk,behavior-macro";
			wait-ms = <40>;
			tap-ms = <40>;
			#binding-cells = <0>;
			bindings
				= <&macro_press   &kp LALT>
				, <&macro_tap     &kp KP_N0 &kp KP_N1 &kp KP_N7 &kp KP_N6>
				, <&macro_release &kp LALT>
				;
		};
        zwspc: zwspc {
            compatible = "zmk,behavior-macro";
			wait-ms = <40>;
			tap-ms = <40>;
			#binding-cells = <0>;
			bindings
				= <&macro_press   &kp LCTRL &kp LSHIFT>
				, <&macro_tap     &kp U>
				, <&macro_release &kp LCTRL &kp LSHIFT>
                , <&macro_tap     &kp KP_N2 &kp KP_N0 &kp KP_N0 &kp B &kp SPACE>
				;
		};
		zwspcold: zwspcold {
			compatible = "zmk,behavior-macro";
			wait-ms = <40>;
			tap-ms = <40>;
			#binding-cells = <0>;
			bindings
				= <&macro_press   &kp LALT>
				, <&macro_tap     &kp KP_N0 &kp KP_N1 &kp KP_N2 &kp KP_N9>
				, <&macro_release &kp LALT>
				;
		};
    };
};

